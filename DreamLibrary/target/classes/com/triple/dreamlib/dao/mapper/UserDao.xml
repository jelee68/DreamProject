<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.triple.dreamlib.dao.UserDao">

 	<insert id="user_addDao">
		INSERT INTO USER_TBL VALUES (#{param1}, #{param2}, #{param3}, #{param4}, #{param5}, #{param6}, '0')
	</insert>
	
 	<select id="loginDao" resultType="com.triple.dreamlib.dto.UserDto">
		select user_id, user_pwd from user_tbl
	</select>
	
</mapper>
